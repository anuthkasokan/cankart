{
  "version": 1,
  "paths": {
    "/videogames/{videogameId}": {
      "name": "/videogames/{videogameId}",
      "lambdaFunction": "videogameapifunc",
      "permissions": {
        "setting": "open"
      }
    },
    "/getList": {
      "name": "/getList",
      "lambdaFunction": "videogamefunc",
      "permissions": {
        "setting": "protected",
        "auth": ["create", "read", "update", "delete"],
        "guest": ["read"]
      }
    },
    "/payment": {
      "name": "/payment",
      "lambdaFunction": "videogameapifunc",
      "permissions": {
        "setting": "protected",
        "auth": ["create", "read", "update", "delete"],
        "guest": ["create", "read", "update", "delete"]
      }
    }
  }
}
